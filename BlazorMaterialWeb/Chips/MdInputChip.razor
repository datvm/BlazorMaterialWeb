@namespace BlazorMaterialWeb

@inherits BaseMdChip

<BaseMdChipRenderer Type="MdChipType.Input"
                    remove-only="@(RemoveOnly)"
                    selected="@(Selected)"
                    avatar="@(Avatar)"
                    href="@(Href)"
                    target="@(Target)"
                    @onchipselected="OnSelectedChanged"
                    @onchipremove="OnRemove"
                    @attributes="AdditionalAttributes"
                    Label="@(Label)"
                    Elevated="@(Elevated)"
                    Disabled="Disabled">
    @(ChildContent)
</BaseMdChipRenderer>