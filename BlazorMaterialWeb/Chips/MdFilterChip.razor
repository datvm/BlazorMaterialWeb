@namespace BlazorMaterialWeb

@inherits BaseMdChip

<BaseMdChipRenderer Type="MdChipType.Filter"
                    removable="@(Removable)"
                    selected="@(Selected)"
                    @onchipselected="OnSelectedChanged"
                    @onchipremove="OnRemove"
                    @attributes="AdditionalAttributes"
                    Label="@(Label)"
                    Elevated="@(Elevated)"
                    Disabled="Disabled">
    @(ChildContent)
</BaseMdChipRenderer>