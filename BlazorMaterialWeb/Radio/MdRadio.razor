@namespace BlazorMaterialWeb
@inherits DefaultMdComponent

<md-radio @attributes="AdditionalAttributes" @ref="el"
          @oncheckedchange="OnCheckedChanged"
          checked="@(Checked)"
          disabled="@(Disabled)"
          value="@(Value)"
          name="@(Name ?? GroupName)">
    @(ChildContent)
</md-radio>
