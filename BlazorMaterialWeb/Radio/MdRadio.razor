@namespace BlazorMaterialWeb

@typeparam TValue where TValue: IConvertible, IEquatable<TValue>;
@inherits DefaultMdComponent

<md-radio @attributes="AdditionalAttributes" @ref="el"
          @onradiochecked="OnDomCheckedChangedAsync"
          checked="@(IsChecked())"
          disabled="@(Disabled)"
          value="@(Value)"
          name="@(Name ?? GroupName)">
    @(ChildContent)
</md-radio>
