@inherits DefaultMdComponent

<DynamicTag Tag="@(TagName)" @attributes="AdditionalAttributes" @bind-DynamicElementReference="el"
            disabled="@(Disabled)"
            aria-label-selected="@(AriaLabelSelected)"
            flip-icon-in-rtl="@(FlipIconInRtl)"
            href="@(Href)"
            selected="@(Selected)"
            target="@(Target)"
            toggle="@(Toggle)"
            type="@(Type)"
            value="@(Value)"
            @oncheckedchange="OnSelectedChanged">
    @(ChildContent)
</DynamicTag>
