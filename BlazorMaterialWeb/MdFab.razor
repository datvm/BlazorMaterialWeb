@inherits DefaultMdComponent

<DynamicTag Tag="@(TagName)" @attributes="AdditionalAttributes" @bind-DynamicElementReference="el"
            label="@(Label)"
            variant="@(Variant.ToString().ToLower())"
            size="@(Size.ToString().ToLower())"
            lowered="@(Lowered)"
            branded="@(Branded)">
    @(ChildContent)
</DynamicTag>